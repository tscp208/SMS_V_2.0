@model IEnumerable<SMS.Models.UserType.UserTypeViewModel>
<div class="row">
    <div class="col-lg-12">
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.SrNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.UserTypeName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.UserTypeDesc)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.SrNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserTypeName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserTypeDesc)
                            </td>
                            <td>
                                @Html.ActionLink(" ", "", "", new { id = item.UserTypeID }, new { @class = "glyphicon glyphicon-edit", style = "font-size:17px;text-decoration:none;", onclick = "EditUserType(" + item.UserTypeID + "); return false;" }) |
                                @Html.ActionLink(" ", "", "", new { userTypeID = item.UserTypeID }, new { @class = "glyphicon glyphicon-remove", style = "font-size:17px;color: #ff000094;text-decoration:none;", onclick = "DeleteUserType(" + item.UserTypeID + "); return false;" })
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
</div>